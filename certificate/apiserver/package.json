{
    "name": "certificate",
    "version": "1.0.0",
    "description": "certificate application implemented in JavaScript",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "fabric-ca-client": "^2.2.4",
        "fabric-network": "^2.2.4",
        "multer": "^1.4.3",
        "pug": "^3.0.2",
        "xls-to-json": "^2.1.0",
        "xls-to-json-lc": "^0.3.4",
        "xlsx-to-json": "^0.3.0",
        "xlsx-to-json-lc": "^0.5.0"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "dotenv": "^10.0.0",
        "eslint": "^8.4.0",
        "mocha": "^9.1.3",
        "nodemon": "^2.0.15",
        "nyc": "^15.1.0",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.3.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
